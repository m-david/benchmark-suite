<?xml version="1.0" encoding="UTF-8"?>
<hazelcast-client
        xsi:schemaLocation="http://www.hazelcast.com/schema/client-config http://www.hazelcast.com/schema/client-config/hazelcast-client-config-3.10.xsd"
        xmlns="http://www.hazelcast.com/schema/client-config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <properties>
        <property name="hazelcast.phone.home.enabled">false</property>
        <property name="hazelcast.logging.type">slf4j</property>
    </properties>

    <group>
        <name>benchmark</name>
        <password>benchmark-pass</password>
    </group>
    <network>
        <cluster-members>
            <address>10.212.1.117:5710</address>
        </cluster-members>
    </network>

    <serialization>
        <portable-factories>
            <portable-factory factory-id="1">
                com.hazelcast.poc.domain.portable.RiskTradePortableFactory
            </portable-factory>
        </portable-factories>
    </serialization>

</hazelcast-client>
